    "workers:notifications": "node dist/workers/notificationWorker.js",
    "workers:analytics": "node dist/workers/analyticsWorker.js",
    "schedule": "node dist/schedulers/index.js"
  },
  "keywords": [
    "food-delivery",
    "multi-module",
    "restaurant-management",
    "delivery-platform",
    "admin-dashboard",
    "hr-system",
    "finance-system"
